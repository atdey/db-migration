<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet author="atish" id="insert-products">
        <preConditions onFail="CONTINUE" onError="CONTINUE">
            <tableExists tableName="products"/>
        </preConditions>
        <insert tableName="products">
            <column name="id" valueNumeric="1"/>
            <column name="name" value="Cheese"/>
            <column name="category" value="Dairy"/>
        </insert>
        <insert tableName="products">
            <column name="id" valueNumeric="2"/>
            <column name="name" value="Milk"/>
            <column name="category" value="Dairy"/>
        </insert>
        <insert tableName="products">
            <column name="id" valueNumeric="3"/>
            <column name="name" value="Butter"/>
            <column name="category" value="Dairy"/>
        </insert>
        <insert tableName="products">
            <column name="id" valueNumeric="4"/>
            <column name="name" value="Popcorn"/>
            <column name="category" value="Snacks"/>
        </insert>
        <insert tableName="products">
            <column name="id" valueNumeric="5"/>
            <column name="name" value="Red Bull"/>
            <column name="category" value="Soft drinks"/>
        </insert>
    </changeSet>
</databaseChangeLog>